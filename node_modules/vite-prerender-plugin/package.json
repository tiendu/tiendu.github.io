    "source-map": "^0.7.4",
    "stack-trace": "^1.0.0-pre2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.8",
    "@types/node": "^20.11.16",
    "dedent": "^1.5.3",
    "prettier": "^2.8.8",
    "prettier-config-rschristian": "^0.1.1",
    "tmp-promise": "^3.0.3",
